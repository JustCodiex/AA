// The main function to test...
int main() {

    // Integer to add and subtract from
    int accum = 0;

    // Define a tuple
    (int, int, int) t1 = (0, 0, 0);

    // Handle matching
    accum = accum + t1 match {
        (0,0) => 2,
        (0,0,0) => 3, // <== This is the one we expect
        (0,0,1) => 4,
        _ => 25
    };

    // Define other tuple
    (bool, bool, int) t2 = (true, false, 25);

    // Another matching
    accum = accum - t2 match {
        (false, false, _) => 4,
        (false, true, _) => 6,
        (true, true, _) => 8,
        (true, false, _) => 60, // <== This is the one we expect
        (true, false, 25) => 61,
        _ => 0
    };

    // Return the result of above
    accum;

};

// Call the function
main();
