(int, int) fib_seq((int, int) sequence, int n) {
	if ((n == 0)) {
		sequence
	} else {
		fib_seq((sequence._2, (sequence._1 + sequence._2)), (n - 1))
	};
}
int fib(int n) {
	(int, int) last = fib_seq((0, 1), n);
	(last._1 + last._2);
}
int sum((int, int) t) {
	(t._1 + t._2);
}
int main() {
	int f1 = fib(4);
	println(f1);
	int f2 = fib(12);
	println(f2);
	sum((f1, f2));
}
