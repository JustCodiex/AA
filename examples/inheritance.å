
public class SuperClass {
    public virtual void SuperMethod() {

    }
    public void PMethod() {

    }
    protected void ProtectedMethod() {

    }
    private void PrivateMethod() {

    }
}

public class BaseClass : SuperClass {

    // inherits PrivateMethod, PMethod
    // inherits and can override SuperMethod

    public override void SuperMethod() {

    }

}

public interface IExample {

    void AbstractMethod();

    void NotAbstract() {
        // has default implementation
    }

}

public implementer : IExample {
    public void AbstractMethod() {

    }
}

public static class staticClassExample : IExample {
    public static void AbstractMethod() { // Must implement a static method inherited from the interface

    }
}
