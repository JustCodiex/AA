int main() {

    bool a = true;
    bool b = true;

    int r = 0;

    if (a && b) { // true
        r = r + 8;
    };

    if (a & b) { // true
        r = r + 29;
    };

    a = false;

    if (a && b) { // false
        r = r + 10;
    };

    if (a & b) { // false
        r = r + 8;
    };

    r;

};

main();
