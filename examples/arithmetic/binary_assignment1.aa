// The main function to test...
int main() {

    // Define the starting value
    var v = 1; // Integer implied

    v += 3; // Add three to v

    v -= 2; // Subtract two

    v *= 4; // Multiplied by four

    v /= 3; // Divide by three

    v %= 2; // Mod by 2

    v; // Return v (v = 0)

};

