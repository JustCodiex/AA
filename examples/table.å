
public Table table_array = {
    "Hello",
    "There!",
    "How",
    "Are",
    "You?",
};

public Table table_dictionary = {
    ["Case A"] = { "Hello", "World" },
    ["Case B"] = 42,
};

public Table table_dot = {
    sub = {
        0, 1, 2
    },
    dub = {
        "A", "B", "C"
    },
}

var x = table_dot.sub[0] // Evaluate to 0
var y = table_dot.sub[1] // Evaluate to 1
var a = table_dot.dub[0] // Evaluate to "A"
